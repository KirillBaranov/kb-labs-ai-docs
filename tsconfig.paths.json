{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@kb-labs/ai-docs-contracts": [
        "packages/ai-docs-contracts/src/index.ts"
      ],
      "@kb-labs/ai-docs-contracts/*": [
        "packages/ai-docs-contracts/src/*"
      ],
      "@kb-labs/cli-core": [
        "../kb-labs-cli/packages/cli-core/src/index.ts"
      ],
      "@kb-labs/cli-core/*": [
        "../kb-labs-cli/packages/cli-core/src/*"
      ],
      "@kb-labs/core-sys": [
        "../kb-labs-core/packages/core-sys/src/index.ts"
      ],
      "@kb-labs/core-sys/*": [
        "../kb-labs-core/packages/core-sys/src/*"
      ],
      "@kb-labs/plugin-manifest": [
        "../kb-labs-plugin/packages/manifest/src/index.ts"
      ],
      "@kb-labs/plugin-manifest/*": [
        "../kb-labs-plugin/packages/manifest/src/*"
      ],
      "@kb-labs/setup-operations": [
        "../kb-labs-setup-engine/packages/setup-operations/src/index.ts"
      ],
      "@kb-labs/setup-operations/*": [
        "../kb-labs-setup-engine/packages/setup-operations/src/*"
      ],
      "@kb-labs/shared-command-kit": [
        "../kb-labs-shared/packages/shared-command-kit/src/index.ts"
      ],
      "@kb-labs/shared-command-kit/*": [
        "../kb-labs-shared/packages/shared-command-kit/src/*"
      ]
    }
  }
}
